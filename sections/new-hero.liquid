{% comment %} Google Fonts for accent text customization {% endcomment %}
{% unless section.settings.use_global_accent_typography %}
  {% if section.settings.accent_font_family != 'inherit' and section.settings.accent_font_family != 'serif' %}
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link
      rel="preconnect"
      href="https://fonts.gstatic.com"
      crossorigin
    >
    {% case section.settings.accent_font_family %}
      {% when 'Georgia Pro' %}
        <link
          href="https://fonts.googleapis.com/css2?family=Georgia+Pro:ital,wght@0,400;0,500;0,600;0,700;1,400;1,500;1,600;1,700&display=swap"
          rel="stylesheet"
        >
      {% when 'Bodoni Moda' %}
        <link
          href="https://fonts.googleapis.com/css2?family=Bodoni+Moda:ital,opsz,wght@0,6..96,400;0,6..96,500;0,6..96,600;0,6..96,700;0,6..96,800;1,6..96,400;1,6..96,500;1,6..96,600;1,6..96,700;1,6..96,800&display=swap"
          rel="stylesheet"
        >
      {% when 'Instrument Serif' %}
        <link href="https://fonts.googleapis.com/css2?family=Instrument+Serif:ital@0;1&display=swap" rel="stylesheet">
      {% when 'DM Serif Text' %}
        <link href="https://fonts.googleapis.com/css2?family=DM+Serif+Text:ital@0;1&display=swap" rel="stylesheet">
      {% when 'Nothing You Could Do' %}
        <link href="https://fonts.googleapis.com/css2?family=Nothing+You+Could+Do&display=swap" rel="stylesheet">
    {% endcase %}
  {% endif %}
{% endunless %}

{{ 'hero.css' | asset_url | stylesheet_tag }}
{% style %}
    /* Rating block container background styles */
    {% for block in section.blocks %}
      {% if block.type == 'rating' and block.settings.enable_container_background %}
        .custom-hero__rating-wrapper[data-block-id="{{ block.id }}"] {
          background-color: {{ block.settings.container_background_color | default: '#f8f9fa' }}
           !important;
          padding: {{ block.settings.container_padding_vertical | default: 12 }}px {{ block.settings.container_padding_horizontal | default: 20 }}px !important;
          border-radius: {{ block.settings.container_border_radius | default: 8 }}px !important;
          display: inline-block !important;
          width: auto !important;
          {% if block.settings.enable_container_border %}
            border: {{ block.settings.container_border_width | default: 1 }}px solid {{ block.settings.container_border_color | default: '#e9ecef' }}
             !important;
          {% endif %}
        }
      {% endif %}
    {% endfor %}

    /* Rating text margin fix */
    .custom-hero__rating-text p {
      margin: 0;
    }

  /* Badge container background */
  {% for block in section.blocks %}
    {% if block.type == 'badge' and block.settings.enable_container_background %}
      .custom-hero__badge-wrapper[data-block-id="{{ block.id }}"] {
        text-align: {{ block.settings.badge_alignment | default: 'left' }} !important;
      }

      .custom-hero__badge-wrapper[data-block-id="{{ block.id }}"] .custom-hero__badge {
        background-color: {{ block.settings.container_background_color | default: '#f8f9fa' }} !important;
        padding: {{ block.settings.container_padding_vertical | default: 12 }}px {{ block.settings.container_padding_horizontal | default: 20 }}px !important;
        border-radius: {{ block.settings.container_border_radius | default: 8 }}px !important;
        display: inline-block !important;
        width: auto !important;
        color: {{ block.settings.badge_text_color | default: '#000000' }} !important;
        {% if block.settings.enable_container_border %}
          border: {{ block.settings.container_border_width | default: 1 }}px solid {{ block.settings.container_border_color | default: '#e9ecef' }} !important;
        {% endif %}
      }
    {% endif %}
  {% endfor %}

  /* Block margin customization */
  {% for block in section.blocks %}
    {% case block.type %}
    {% when 'badge' %}
    .custom-hero__badge-wrapper[data-block-id="{{ block.id }}"] {
      margin-top: {{ block.settings.margin_top | default: 0 }}px !important;
      margin-bottom: {{ block.settings.margin_bottom | default: 16 }}px !important;
      {% unless block.settings.enable_container_background %}
        text-align: {{ block.settings.badge_alignment | default: 'left' }} !important;
      {% endunless %}
    }
    
    {% unless block.settings.enable_container_background %}
      .custom-hero__badge-wrapper[data-block-id="{{ block.id }}"] .custom-hero__badge {
        color: {{ block.settings.badge_text_color | default: '#000000' }} !important;
      }
    {% endunless %}
  {% when 'rating' %}
  .custom-hero__rating-wrapper[data-block-id="{{ block.id }}"] {
    margin-top: {{ block.settings.margin_top | default: 0 }}px !important;
    margin-bottom: {{ block.settings.margin_bottom | default: 16 }}px !important;
  }
  
  .custom-hero__rating-wrapper[data-block-id="{{ block.id }}"] .custom-hero__rating-text {
    color: {% if block.settings.use_theme_colors %}{{ settings.global_section_text_color }}{% else %}{{ block.settings.rating_text_color | default: '#000000' }}{% endif %} !important;
    font-size: {{ block.settings.rating_text_size | default: 14 }}px !important;
  }
  
  .custom-hero__rating-wrapper[data-block-id="{{ block.id }}"] .custom-hero__rating-stars {
    color: {% if block.settings.use_theme_colors %}{{ settings.global_section_accent_1_color }}{% else %}{{ block.settings.star_color | default: '#ffc107' }}{% endif %} !important;
    font-size: {{ block.settings.rating_stars_size | default: 16 }}px !important;
  }
  
  .custom-hero__rating-wrapper[data-block-id="{{ block.id }}"] .hero-star-icon {
    color: {% if block.settings.use_theme_colors %}{{ settings.global_section_accent_1_color }}{% else %}{{ block.settings.star_color | default: '#ffc107' }}{% endif %} !important;
    fill: {% if block.settings.use_theme_colors %}{{ settings.global_section_accent_1_color }}{% else %}{{ block.settings.star_color | default: '#ffc107' }}{% endif %} !important;
    width: {{ block.settings.rating_stars_size | default: 16 }}px !important;
    height: {{ block.settings.rating_stars_size | default: 16 }}px !important;
  }
  
  .custom-hero__rating-wrapper[data-block-id="{{ block.id }}"] .custom-hero__rating-stars svg {
    width: {{ block.settings.rating_stars_size | default: 16 }}px !important;
    height: {{ block.settings.rating_stars_size | default: 16 }}px !important;
  }
{% when 'heading' %}
.section-{{ section.id }}.custom-hero .custom-hero__title[data-block-id="{{ block.id }}"],
.section-{{ section.id }}.custom-hero .custom-hero__title.section-heading[data-block-id="{{ block.id }}"],
.custom-hero__content-wrapper .custom-hero__title[data-block-id="{{ block.id }}"] {
  margin-top: {{ block.settings.margin_top | default: 0 }}px !important;
  margin-bottom: {{ block.settings.margin_bottom | default: 16 }}px !important;
  color: {% if block.settings.use_theme_colors %}{{ settings.global_section_text_color }}
{% else %}{{ block.settings.heading_color | default: section.settings.heading_color }}
{% endif %}
 !important;
  font-size: {{ block.settings.heading_size | default: section.settings.heading_size }}px !important;
  line-height: {{ block.settings.heading_line_height | default: section.settings.heading_line_height }} !important;
  text-align: {{ section.settings.desktop_content_alignment | default: 'left' }} !important;
}

@media (max-width: 768px) {
  .section-{{ section.id }}.custom-hero .custom-hero__title[data-block-id="{{ block.id }}"],
  .section-{{ section.id }}.custom-hero .custom-hero__title.section-heading[data-block-id="{{ block.id }}"],
  .custom-hero__content-wrapper .custom-hero__title[data-block-id="{{ block.id }}"] {
    font-size: {{ block.settings.heading_size_mobile | default: section.settings.heading_size_mobile }}px !important;
    line-height: {{ block.settings.heading_line_height_mobile | default: section.settings.heading_line_height_mobile }} !important;
    text-align: {{ section.settings.mobile_content_alignment | default: section.settings.desktop_content_alignment | default: 'left' }} !important;
  }
}

/* Accent text styling for this heading block */
.section-{{ section.id }}.custom-hero .custom-hero__title[data-block-id="{{ block.id }}"] .section-accent-text {
  {% if block.settings.use_theme_colors %}
    /* Use theme colors - check for global gradient */
    {% if settings.global_accent_text_gradient_dark != blank %}
      background: {{ settings.global_accent_text_gradient_dark }} !important;
      -webkit-background-clip: text !important;
      -webkit-text-fill-color: transparent !important;
      background-clip: text !important;
      text-fill-color: transparent !important;
    {% else %}
      color: {{ settings.global_section_accent_1_color | default: '#ef4a65' }} !important;
    {% endif %}
  {% else %}
    /* Theme colors OFF - use block settings */
    {% if block.settings.use_accent_gradient and block.settings.accent_gradient != blank %}
      background: {{ block.settings.accent_gradient }} !important;
      -webkit-background-clip: text !important;
      -webkit-text-fill-color: transparent !important;
      background-clip: text !important;
      text-fill-color: transparent !important;
    {% else %}
      color: {{ block.settings.accent_color | default: '#ef4a65' }} !important;
    {% endif %}
  {% endif %}
}

{% when 'subtitle' %}
.section-{{ section.id }}.custom-hero .custom-hero__subtitle[data-block-id="{{ block.id }}"],
.custom-hero__content-wrapper .custom-hero__subtitle[data-block-id="{{ block.id }}"] {
  margin-top: {{ block.settings.margin_top | default: 0 }}px !important;
  margin-bottom: {{ block.settings.margin_bottom | default: 24 }}px !important;
  color: {{ block.settings.subtitle_text_color | default: '#666666' }} !important;
  font-weight: {{ block.settings.subtitle_font_weight | default: '400' }} !important;
  font-size: {{ block.settings.subtitle_font_size_mobile | default: 16 }}px !important;
  line-height: {{ block.settings.subtitle_line_height | default: 1.5 }} !important;
}

.section-{{ section.id }}.custom-hero .custom-hero__subtitle[data-block-id="{{ block.id }}"] p,
.custom-hero__content-wrapper .custom-hero__subtitle[data-block-id="{{ block.id }}"] p {
  color: {{ block.settings.subtitle_text_color | default: '#666666' }} !important;
  font-weight: {{ block.settings.subtitle_font_weight | default: '400' }} !important;
  font-size: {{ block.settings.subtitle_font_size_mobile | default: 16 }}px !important;
  line-height: {{ block.settings.subtitle_line_height | default: 1.5 }} !important;
}

@media screen and (min-width: 769px) {
  .section-{{ section.id }}.custom-hero .custom-hero__subtitle[data-block-id="{{ block.id }}"],
  .custom-hero__content-wrapper .custom-hero__subtitle[data-block-id="{{ block.id }}"],
  .section-{{ section.id }}.custom-hero .custom-hero__subtitle[data-block-id="{{ block.id }}"] p,
  .custom-hero__content-wrapper .custom-hero__subtitle[data-block-id="{{ block.id }}"] p {
    font-size: {{ block.settings.subtitle_font_size_desktop | default: 18 }}px !important;
  }
}
{% when 'benefits' %}
.custom-hero__benefits[data-block-id="{{ block.id }}"] {
  margin-top: {{ block.settings.margin_top | default: 0 }}px !important;
  margin-bottom: {{ block.settings.margin_bottom | default: 24 }}px !important;
}

.custom-hero__benefits[data-block-id="{{ block.id }}"] .custom-hero__benefit-item {
  color: {% if block.settings.use_theme_colors %}{{ settings.global_section_text_color }}
{% else %}{{ block.settings.benefit_text_color | default: section.settings.benefit_text_color }}
{% endif %};
  font-size: {{ section.settings.benefit_font_size }}px !important;
  {% if section.settings.desktop_content_alignment == 'center' %}
    justify-content: center;
  {% elsif section.settings.desktop_content_alignment == 'right' %}
    justify-content: flex-end;
  {% else %}
    justify-content: flex-start;
  {% endif %}
}

.custom-hero__benefits[data-block-id="{{ block.id }}"] .custom-hero__benefit-icon {
  color: {% if block.settings.use_theme_colors %}{{ settings.global_section_accent_1_color }}
{% else %}{{ block.settings.benefit_icon_color | default: section.settings.benefit_icon_color }}
{% endif %};
  font-size: {{ section.settings.benefit_icon_size }}px !important;
}

.custom-hero__benefits[data-block-id="{{ block.id }}"] .custom-hero__benefit-icon svg {
  width: {{ section.settings.benefit_icon_size }}px !important;
  height: {{ section.settings.benefit_icon_size }}px !important;
  fill: {% if block.settings.use_theme_colors %}{{ settings.global_section_accent_1_color }}
{% else %}{{ block.settings.benefit_icon_color | default: section.settings.benefit_icon_color }}
{% endif %};
}

.custom-hero__benefits[data-block-id="{{ block.id }}"] .custom-hero__benefit-icon img {
  width: {{ section.settings.benefit_icon_size }}px !important;
  height: {{ section.settings.benefit_icon_size }}px !important;
  object-fit: contain !important;
  display: block !important;
}
{% when 'button' %}
.custom-hero__button-wrapper[data-block-id="{{ block.id }}"] {
  margin-top: {{ block.settings.margin_top | default: 0 }}px !important;
  margin-bottom: {{ block.settings.margin_bottom | default: 24 }}px !important;
  {% if block.settings.full_width %}
    width: 100% !important;
  {% endif %}
}
{% if block.settings.full_width %}
  .section-{{ section.id }}.custom-hero .custom-hero__button-wrapper[data-block-id="{{ block.id }}"],
  .custom-hero__content-wrapper .custom-hero__button-wrapper[data-block-id="{{ block.id }}"] {
    width: 100% !important;
    display: block !important;
    overflow: hidden !important;
    max-width: 100% !important;
  }
  .section-{{ section.id }}.custom-hero .custom-hero__button-wrapper[data-block-id="{{ block.id }}"] .custom-hero__button,
  .section-{{ section.id }}.custom-hero .custom-hero__button-wrapper[data-block-id="{{ block.id }}"] .theme-button,
  .custom-hero__content-wrapper .custom-hero__button-wrapper[data-block-id="{{ block.id }}"] .custom-hero__button,
  .custom-hero__content-wrapper .custom-hero__button-wrapper[data-block-id="{{ block.id }}"] .theme-button {
    width: 100% !important;
    display: block !important;
    text-align: center !important;
    max-width: 100% !important;
    box-sizing: border-box !important;
  }
{% endif %}
{% if block.settings.full_width_mobile_only %}
  @media (max-width: 768px) {
    .section-{{ section.id }}.custom-hero .custom-hero__button-wrapper[data-block-id="{{ block.id }}"],
    .custom-hero__content-wrapper .custom-hero__button-wrapper[data-block-id="{{ block.id }}"] {
      width: 100% !important;
      display: block !important;
    }
    .custom-hero .custom-hero__button-wrapper[data-block-id="{{ block.id }}"] .custom-hero__button,
    .custom-hero .custom-hero__button-wrapper[data-block-id="{{ block.id }}"] .theme-button,
    .custom-hero__content-wrapper .custom-hero__button-wrapper[data-block-id="{{ block.id }}"] .custom-hero__button,
    .custom-hero__content-wrapper .custom-hero__button-wrapper[data-block-id="{{ block.id }}"] .theme-button {
      width: 100% !important;
      display: block !important;
      text-align: center !important;
      max-width: none !important;
    }
  }
{% endif %}
{% when 'guarantee' %}
.custom-hero__guarantee[data-block-id="{{ block.id }}"] {
  margin-top: {{ block.settings.margin_top | default: 0 }}px !important;
  margin-bottom: {{ block.settings.margin_bottom | default: 0 }}px !important;
  text-align: {{ block.settings.guarantee_alignment_desktop | default: 'left' }} !important;
  justify-content: {% case block.settings.guarantee_alignment_desktop %}
    {% when 'center' %}center
    {% when 'right' %}flex-end
    {% else %}flex-start
  {% endcase %} !important;
}

@media screen and (max-width: 768px) {
  .custom-hero__guarantee[data-block-id="{{ block.id }}"] {
    text-align: {{ block.settings.guarantee_alignment_mobile | default: block.settings.guarantee_alignment_desktop | default: 'left' }} !important;
    justify-content: {% case block.settings.guarantee_alignment_mobile %}
      {% when 'center' %}center
      {% when 'right' %}flex-end
      {% else %}flex-start
    {% endcase %} !important;
  }
}

.custom-hero__guarantee[data-block-id="{{ block.id }}"] .custom-hero__guarantee-text {
  color: {{ block.settings.guarantee_text_color | default: '#000000' }} !important;
  font-size: {{ block.settings.guarantee_font_size | default: 14 }}px !important;
  font-weight: {{ block.settings.guarantee_font_weight | default: '400' }} !important;
}

.custom-hero__guarantee[data-block-id="{{ block.id }}"] .custom-hero__guarantee-icon {
  {% if block.settings.guarantee_icon_color != blank %}
    color: {{ block.settings.guarantee_icon_color }} !important;
  {% endif %}
  {% if block.settings.guarantee_icon_size != blank %}
    width: {{ block.settings.guarantee_icon_size }}px !important;
    height: {{ block.settings.guarantee_icon_size }}px !important;
  {% endif %}
}

.custom-hero__guarantee[data-block-id="{{ block.id }}"] .custom-hero__guarantee-icon svg {
  {% if block.settings.guarantee_icon_color != blank %}
    fill: {{ block.settings.guarantee_icon_color }} !important;
    stroke: {{ block.settings.guarantee_icon_color }} !important;
  {% endif %}
  {% if block.settings.guarantee_icon_size != blank %}
    width: {{ block.settings.guarantee_icon_size }}px !important;
    height: {{ block.settings.guarantee_icon_size }}px !important;
  {% endif %}
}

.custom-hero__guarantee[data-block-id="{{ block.id }}"] .custom-hero__guarantee-icon-image {
  {% if block.settings.guarantee_icon_size != blank %}
    width: {{ block.settings.guarantee_icon_size }}px !important;
    height: {{ block.settings.guarantee_icon_size }}px !important;
  {% endif %}
  object-fit: contain !important;
  display: block !important;
}
{% endcase %}
{% endfor %}

/* Desktop Background Image Styles */
@media screen and (min-width: 769px) {
  .section-{{ section.id }}.custom-hero {
    {% if section.settings.use_background_image and section.settings.hero_background_image != blank %}
      {% comment %} When background image is present, layer image above gradient {% endcomment %}
      {% if section.settings.use_theme_colors %}
        {% if settings.global_section_use_gradient_background %}
          background: url('{{ section.settings.hero_background_image | image_url: width: 2560 }}'), {{ settings.global_section_gradient_background }} !important;
        {% else %}
          background: url('{{ section.settings.hero_background_image | image_url: width: 2560 }}'), {{ settings.global_section_background_color }} !important;
        {% endif %}
      {% elsif section.settings.enable_gradient %}
        background: url('{{ section.settings.hero_background_image | image_url: width: 2560 }}'), {{ section.settings.section_gradient_background }} !important;
      {% else %}
        background: url('{{ section.settings.hero_background_image | image_url: width: 2560 }}'), {{ section.settings.section_bg }} !important;
      {% endif %}
      background-size: {{ section.settings.background_image_size | default: 'cover' }}, cover !important;
      background-position: {{ section.settings.background_image_position | default: 'center center' }}, center !important;
      background-repeat: no-repeat, no-repeat !important;
      background-attachment: {{ section.settings.background_image_attachment | default: 'scroll' }}, scroll !important;
    {% endif %}
  }
}

/* Base Hero Section Styles */
.section-{{ section.id }}.custom-hero {
  {% unless section.settings.use_background_image and section.settings.hero_background_image != blank %}
    {% if section.settings.use_theme_colors %}
      {% if settings.global_section_use_gradient_background %}
        background: {{ settings.global_section_gradient_background }};
      {% else %}
        background-color: {{ settings.global_section_background_color }};
      {% endif %}
    {% elsif section.settings.enable_gradient %}
      background: {{ section.settings.section_gradient_background }};
    {% else %}
      background-color: {{ section.settings.section_bg }};
    {% endif %}
  {% endunless %}
  padding: {{ section.settings.section_padding_vertical }}px {{ section.settings.section_padding_horizontal }}px;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  {% if section.settings.use_background_image and section.settings.hero_background_image != blank and section.settings.background_overlay_enable %}
    position: relative;
  {% endif %}
  {% if section.settings.enable_top_border %}
    border-top: {{ section.settings.top_border_width | default: 1 }}px {{ section.settings.top_border_style | default: 'solid' }} {{ section.settings.top_border_color | default: '#e5e5e5' }};
  {% endif %}
  {% if section.settings.enable_bottom_border %}
    border-bottom: {{ section.settings.bottom_border_width | default: 1 }}px {{ section.settings.bottom_border_style | default: 'solid' }} {{ section.settings.bottom_border_color | default: '#e5e5e5' }};
  {% endif %}
}

{% if section.settings.use_background_image and section.settings.hero_background_image != blank and section.settings.background_overlay_enable %}
/* Background overlay */
.section-{{ section.id }}.custom-hero::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: {{ section.settings.background_overlay_color | default: 'rgba(0,0,0,0.3)' }};
  opacity: {{ section.settings.background_overlay_opacity | default: 30 | divided_by: 100.0 }};
  z-index: 1;
}

.section-{{ section.id }}.custom-hero .custom-hero__container {
  position: relative;
  z-index: 2;
}
{% endif %}

{% if section.settings.mobile_background_image != blank %}
@media screen and (max-width: 768px) {
  .section-{{ section.id }}.custom-hero {
    padding: {{ section.settings.section_padding_vertical_mobile }}px {{ section.settings.section_padding_horizontal_mobile }}px;
    background: url('{{ section.settings.mobile_background_image | image_url: width: 1024 }}') !important;
    background-size: {{ section.settings.background_image_size | default: 'cover' }} !important;
    background-position: {{ section.settings.background_image_position | default: 'center center' }} !important;
    background-repeat: no-repeat !important;
    position: relative;
    min-height: 400px; /* Fallback minimum height */
  }
  
  /* Pseudo-element placeholder to maintain space for mobile background image */
  .section-{{ section.id }}.custom-hero::after {
    content: '';
    display: block;
    width: 100%;
    {% assign mobile_aspect_ratio = section.settings.mobile_background_image.aspect_ratio | default: 1.5 %}
    {% assign spacing_multiplier = section.settings.mobile_bg_spacing_multiplier | default: 1.0 %}
    {% assign padding_percentage = 100.0 | divided_by: mobile_aspect_ratio | times: spacing_multiplier %}
    padding-bottom: {{ padding_percentage }}%;
    pointer-events: none;
  }
  
  .section-{{ section.id }}.custom-hero .custom-hero__container {
    position: absolute;
    left: 0;
    right: 0;
    display: flex;
    flex-direction: column;
    padding: {{ section.settings.section_padding_vertical_mobile }}px {{ section.settings.section_padding_horizontal_mobile }}px;
    
    {% case section.settings.mobile_bg_content_position %}
      {% when 'top' %}
        top: 0;
        bottom: auto;
        justify-content: flex-start;
      {% when 'bottom' %}
        top: auto;
        bottom: 0;
        justify-content: flex-end;
      {% when 'space-between' %}
        top: 0;
        bottom: 0;
        justify-content: space-between;
      {% else %}
        top: 0;
        bottom: 0;
        justify-content: center;
    {% endcase %}
  }
}
{% else %}
@media screen and (max-width: 768px) {
  .section-{{ section.id }}.custom-hero {
    padding: {{ section.settings.section_padding_vertical_mobile }}px {{ section.settings.section_padding_horizontal_mobile }}px;
  }
}
{% endif %}

  .custom-hero__container {
    {% if section.settings.contain_section %}
      max-width: {{ section.settings.container_max_width }}px;
      margin: 0 auto;
      padding: {{ section.settings.container_padding_top }}px {{ section.settings.container_padding_right }}px {{ section.settings.container_padding_bottom }}px {{ section.settings.container_padding_left }}px;
      border-radius: {{ section.settings.border_radius }}px;
      position: relative;
      {% if section.settings.container_background_image != blank %}
        background-image: url('{{ section.settings.container_background_image | image_url: width: 1600 }}');
        background-size: {{ section.settings.container_bg_size | default: 'cover' }};
        background-position: {{ section.settings.container_bg_position | default: 'center' }};
        background-repeat: {% if section.settings.container_bg_repeat %}repeat{% else %}no-repeat{% endif %};
