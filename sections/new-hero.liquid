/* Button Block Styling - Now at Block Level */
{% for block in section.blocks %}
  {% if block.type == 'button' %}
    .custom-hero__button-wrapper[data-block-id="{{ block.id }}"] .custom-hero__button {
      display: inline-block;
      width: 100% !important;
      padding: var(--global-button-padding-y, 12px) 20px !important;
      letter-spacing: var(--global-button-letter-spacing, 1px) !important;
      cursor: pointer;
      transition: all 0.3s ease;
      text-decoration: none !important;
      text-align: center;
      box-shadow: none !important;

      {% if block.settings.use_global_button_styling %}
        /* Use Global Button Styling */
        font-size: var(--global-button-font-size, 14px) !important;
        font-weight: var(--global-button-font-weight, 700) !important;
        text-transform: var(--global-button-text-transform, none) !important;
        border-radius: var(--global-button-radius, 8px) !important;
        {% if settings.enable_button_gradient %}
        background: linear-gradient({{ settings.button_gradient_direction }}, {{ settings.button_gradient_start }}, {{ settings.button_gradient_end }}) !important;
        {% else %}
        background-color: var(--global-button-bg-color, #EF4A65) !important;
        {% endif %}
        color: var(--global-button-text-color, #FFFFFF) !important;
        border-left: var(--global-button-border-width, 2px) solid var(--global-button-border-color, #EF4A65) !important;
        border-right: var(--global-button-border-width, 2px) solid var(--global-button-border-color, #EF4A65) !important;
        border-top: var(--global-button-border-width, 2px) solid var(--global-button-border-color, #EF4A65) !important;
        border-bottom: var(--global-button-bottom-border-width, 4px) solid var(--global-button-bottom-border-color, #000000) !important;
        border-bottom-style: solid !important;
      {% else %}
        /* Use Block-Specific Custom Styling - Override ALL global styles */
        background: {{ block.settings.button_bg_color | default: '#EF4A65' }} !important;
        background-image: none !important;
        color: {{ block.settings.button_text_color | default: '#FFFFFF' }} !important;
        border-radius: {{ block.settings.button_border_radius | default: 8 }}px !important;
        font-size: {{ block.settings.button_font_size | default: 16 }}px !important;
        font-weight: 700 !important;
        text-transform: none !important;
        border: {{ block.settings.button_border_width | default: 0 }}px solid {{ block.settings.button_border_color | default: '#000000' }} !important;
        border-bottom-width: {{ block.settings.button_bottom_border_width | default: 0 }}px !important;
        border-bottom-style: solid !important;
        border-bottom-color: {{ block.settings.button_bottom_border_color | default: '#000000' }} !important;
      {% endif %}
    }

    .custom-hero__button-wrapper[data-block-id="{{ block.id }}"] .custom-hero__button:hover {
      {% if block.settings.use_global_button_styling %}
        /* Global Button Hover State */
        {% if settings.enable_button_gradient %}
        background: linear-gradient({{ settings.button_gradient_direction }}, {{ settings.button_gradient_hover_start }}, {{ settings.button_gradient_hover_end }}) !important;
        {% else %}
        background-color: var(--global-button-hover-bg-color, #D63953) !important;
        {% endif %}
        color: var(--global-button-hover-text-color, #FFFFFF) !important;
        transform: translateY(-1px);
        box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1) !important;
        border-left: var(--global-button-border-width, 2px) solid var(--global-button-border-color, #EF4A65) !important;
        border-right: var(--global-button-border-width, 2px) solid var(--global-button-border-color, #EF4A65) !important;
        border-top: var(--global-button-border-width, 2px) solid var(--global-button-border-color, #EF4A65) !important;
        border-bottom: var(--global-button-bottom-border-width, 4px) solid var(--global-button-bottom-border-color, #000000) !important;
        border-bottom-style: solid !important;
      {% else %}
        /* Custom Block Hover State - Override ALL global hover styles */
        background: {{ block.settings.button_hover_bg_color | default: '#D93D54' }} !important;
        background-image: none !important;
        color: {{ block.settings.button_text_color | default: '#FFFFFF' }} !important;
        transform: translateY(-1px);
        box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1) !important;
        border: {{ block.settings.button_border_width | default: 0 }}px solid {{ block.settings.button_border_color | default: '#000000' }} !important;
        border-bottom-width: {{ block.settings.button_bottom_border_width | default: 0 }}px !important;
        border-bottom-style: solid !important;
        border-bottom-color: {{ block.settings.button_bottom_border_color | default: '#000000' }} !important;
      {% endif %}
    }
  {% endif %}
{% endfor %}

  .custom-hero .custom-hero__review-count {
