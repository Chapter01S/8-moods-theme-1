{%- comment -%}
  New Payment Icons System - Text Input Based
  User can type payment brand names and corresponding SVG icons will appear
  Supported brands: visa, mastercard, american_express, apple_pay, afterpay, dankort, diners_club, discover
{%- endcomment -%}

{%- liquid
  assign payment_list = block.settings.custom_payment_icons | strip | replace: '\n', '' | replace: '\r', '' | replace: '\t', '' | downcase
  assign payment_brands = payment_list | split: ','

  assign icon_height = block.settings.icon_height | default: 22
  assign icon_spacing = block.settings.icon_spacing | default: 6
  assign alignment = block.settings.alignment | default: 'center'
  assign margin_top = block.settings.margin_top | default: 8
  assign margin_bottom = block.settings.margin_bottom | default: 8
-%}

<div
  class="payment-icons-container payment-icons-{{ block.id }}"
  id="payment-icons-{{ block.id }}"
  style="
    text-align: {{ alignment }};
    margin-top: 2px;
    margin-bottom: 2px;
  "
>
  <div
    class="payment-icons-wrapper"
    style="
      display: inline-flex;
      flex-wrap: wrap;
      justify-content: center;
      align-items: center;
    "
  >
    {%- for brand in payment_brands -%}
      {%- assign brand_clean = brand | strip | downcase -%}
      {%- unless brand_clean == blank -%}
        <div class="payment-icon">
          {%- case brand_clean -%}
