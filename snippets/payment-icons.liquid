<style>
  #payment-icons-{{ block.id }}.payment-icons-container {
    width: 100%;
    margin: 0;
    padding: 0;
  }

  #payment-icons-{{ block.id }} .payment-icons-wrapper {
    margin: 0 auto;
    gap: {{ icon_spacing }}px !important;
  }

  #payment-icons-{{ block.id }} .payment-icon {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
    opacity: 1 !important;
    visibility: visible !important;
    height: {{ icon_height }}px !important;
  }

  #payment-icons-{{ block.id }} .payment-icon svg {
    max-height: 100%;
    width: auto;
    min-width: 30px;
    display: block;
    opacity: 1 !important;
    visibility: visible !important;
    height: {{ icon_height }}px !important;
  }

  @media screen and (max-width: 749px) {
    #payment-icons-{{ block.id }} .payment-icons-wrapper {
      gap: {{ icon_spacing | divided_by: 2 }}px !important;
    }

    #payment-icons-{{ block.id }} .payment-icon {
      height: {{ icon_height | times: 0.8 }}px !important;
    }

    #payment-icons-{{ block.id }} .payment-icon svg {
      height: {{ icon_height | times: 0.8 }}px !important;
    }
  }
</style>