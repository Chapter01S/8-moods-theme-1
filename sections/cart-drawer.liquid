{% comment %}
  üí° Cart Drawer Structure & Organization

  ‚û§ Each part of the cart drawer is rendered via separate snippets.
    Example:
      - {% render 'cart-header' %}             ‚Üí file: snippets/cart-header.liquid
      - {% render 'cart-items' %}              ‚Üí file: snippets/cart-items.liquid
      - {% render 'cart-discount-banner' %}    ‚Üí file: snippets/cart-discount-banner.liquid
      - {% render 'cart-social_proof_bar' %}   ‚Üí file: snippets/cart-social_proof_bar.liquid
      - {% render 'cart-drawer-upsell' %}      ‚Üí file: snippets/cart-drawer-upsell.liquid

  ‚û§ Cart-specific styles are centralized in a dedicated snippet.
    Example:
      - {% render 'cart-drawer_css' %} ‚Üí file: snippets/cart-drawer_css.liquid

    This file defines all dynamic CSS variables (colors, backgrounds, etc.)
    and is injected inside a <style> tag to allow Liquid variables to be interpreted by Shopify.


  ‚û§ Necessary script rendered via snippets
      - {% render 'cart-drawer-script' %} ‚Üí file: snippets/cart-drawer-script.liquid

  üîÅ Tip: Use consistent naming like `cart-nameOfBlock` for your snippets
          to keep your cart components modular and easy to navigate.
{% endcomment %}

{{ 'quantity-popover.css' | asset_url | stylesheet_tag }}
{{ 'component-card.css' | asset_url | stylesheet_tag }}

<script src="{{ 'cart.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'cart-drawer.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'quantity-popover.js' | asset_url }}" defer="defer"></script>

{% liquid
  assign enabled_upsell_block = section.settings.use_upsell_block
  assign upsell_carousel_mode = section.settings.upsell_carousel_mode | default: false
%}

{% if section.settings.cart_customization_mode %}
  <script src="{{ 'cart_customization_mode.js' | asset_url }}" defer="defer"></script>
{% endif %}

{% render 'cart-drawer-script' %}

{% comment %}  Load the script to have shippingProtectionId Everywhere {% endcomment %}
{% for block in section.blocks %}
  {% if block.type == 'cart_shipping_protection' %}
    {% assign shipping_protection_id = block.settings.product_shipping_protection.selected_or_first_available_variant.id %}
    <script>
      const shippingProductId = '{{ shipping_protection_id }}';
    </script>
  {% endif %}
  {% if block.type == 'cart_progress_bar' %}
    {% assign progress_bar_free_id = block.settings.progress_bar_free_product.first_available_variant.id %}
    {% assign progress_bar_free_title = block.settings.product_free_custom_id_title %}

    <script>
      const progressBarProductId = '{{ block.settings.progress_bar_free_product.first_available_variant.id }}';

      document.addEventListener('DOMContentLoaded', () => {
        // Check if there's a premium-attachment-kit block with product-specific free gifts
        const premiumKit = document.querySelector('.premium-attachment-kit');
        let freeProductId = progressBarProductId;
        
        if (premiumKit) {
          const blockFreeGiftIds = premiumKit.getAttribute('data-free-gift-product-ids');
          if (blockFreeGiftIds && blockFreeGiftIds.trim() !== '') {
            const freeGiftIds = blockFreeGiftIds.split(',').filter(id => id.trim() !== '');
            if (freeGiftIds.length > 0) {
              freeProductId = freeGiftIds[0];
              console.log('Using free gift from premium-attachment-kit block:', freeProductId);
            }
          }
        }
        
        const input = document.querySelector('.free-product-form .product-variant-id');
        if (input) {
          input.value = freeProductId;
          console.log('Free product form updated with ID:', freeProductId, 'from:', premiumKit ? 'block' : 'cart_progress_bar');
        }
      });
    </script>
  {% endif %}
{% endfor %}

<cart-drawer class="drawer{% if cart == empty %} is-empty{% endif %}{% if section.settings.cart_customization_mode %} customization-mode{% endif %}">
  <div id="CartDrawer" class="gb-cart-drawer-lb cart-drawer">
    <div id="CartDrawer-Overlay" class="cart-drawer__overlay"></div>
    <div
      class="drawer__inner gradient"
      role="dialog"
      aria-modal="true"
      aria-label="{{ 'sections.cart.title' | t }}"
      tabindex="-1"
      style="background: var(--cart-drawer-background-gradient); background-color: var(--cart-drawer-primary-background-color); color: var(--cart-drawer-primary-text-color);"
    >
      {%- if cart == empty -%}
        {% render 'cart-empty' %}
      {%- else -%}
        {% render 'cart-header' %}
      {%- endif -%}
      <div class="cart-drawer-header-blocks">
        {% for block in section.blocks %}
          {% case block.type %}
              {% comment %} Discount Banner - Start {% endcomment %}
            {% when 'cart-discount-banner' %}
              {% if cart != empty %}
                {% render 'cart-discount-banner', block: block %}
              {% endif %}

              {% comment %} Discount Banner - End {% endcomment %}
            {% when 'cart-social_proof_bar' %}
              {% if cart != empty %}
                {% render 'cart-social_proof_bar', block: block %}
              {% endif %}
            {% when 'cart_timer_bar' %}
              {% if cart != empty %}
                {% render 'cart-timer-bar', block: block %}
              {% endif %}
            {% when 'cart_image_banner' %}
              {% if cart != empty %}
                {% render 'cart-image-banner', block: block %}
              {% endif %}
            {% when 'cart_promo_banner' %}
              {% if cart != empty %}
                {% render 'cart-promo-banner', block: block %}
              {% endif %}
          {% endcase %}
        {% endfor %}
      </div>

      <cart-drawer-items
        {% if cart == empty %}
          class=" is-empty"
        {% endif %}
      >
        <div class="scrollable-contents {% if upsell_carousel_mode %}upsell_slider{% endif %}">
          <form
            action="{{ routes.cart_url }}"
            id="CartDrawer-Form"
            class="cart__contents cart-drawer__form"
            method="post"
          >
            {% for block in section.blocks %}
              {% case block.type %}
                {% when 'cart_progress_bar' %}
                  {% render 'cart-drawer_pgb_style', block: block %}
                  {% render 'cart-progress-bar', block: block %}
              {% endcase %}
            {% endfor %}
            <div id="CartDrawer-CartItems" class="drawer__contents js-contents">
              {%- if cart != empty -%}
                {% render 'cart-items',
                  shipping_protection_id: shipping_protection_id,
                  progress_bar_free_id: progress_bar_free_id,
                  progress_bar_free_title: progress_bar_free_title
                %}
              {%- endif -%}

              <p id="CartDrawer-LiveRegionText" class="visually-hidden" role="status"></p>
              <p id="CartDrawer-LineItemStatus" class="visually-hidden" aria-hidden="true" role="status">
                {{ 'accessibility.loading' | t }}
              </p>
            </div>

            <div id="CartDrawer-CartErrors" role="alert"></div>
          </form>

          {%- assign upsell_count = 0 -%}

          {%- comment -%} Render upsell header if exists {%- endcomment -%}
          {%- for block in section.blocks -%}
            {%- if block.type == 'upsell_header' and cart != empty -%}
              {%- liquid 
                assign upsell_blocks = section.blocks | where: 'type', 'cart_upsell'
                assign total_upsell = upsell_blocks.size

                for upsell_block in upsell_blocks
                  assign this = upsell_block.settings
                  assign upsell_id = this.upsell_product_1.id
                  assign is_on_cart = cart.items | where: 'product_id', upsell_id | first

                  if is_on_cart
                    assign total_upsell = total_upsell | minus: 1
                  endif
                endfor
              -%}

              {%- if total_upsell > 0 -%}
                <style>
                  .upsell-header-text strong,
                  .upsell-header-text b {
                    font-weight: 700 !important;
                  }
                  .upsell-header-text p {
                    margin: 0;
                    padding: 0;
                  }

                  {% if block.settings.show_divider %}
                  .upsell-header-wrapper-{{ block.id }} .upsell-header-with-divider {
                    display: flex;
                    align-items: center;
                    width: 100%;
                    gap: {{ block.settings.divider_spacing | default: 12 }}px;
                    margin-top: {{ block.settings.header_margin_top | default: 8 }}px !important;
                    margin-bottom: {{ block.settings.header_margin_bottom | default: 8 }}px !important;
                    padding-left: 13px !important;
                    padding-right: 13px !important;
                  }

                  .upsell-header-wrapper-{{ block.id }} .upsell-header-with-divider .upsell-header-text {
                    flex: 0 0 auto;
                    width: auto;
                    max-width: none;
                  }

                  .upsell-header-wrapper-{{ block.id }} .divider-line {
                    flex: 1;
                    height: {{ block.settings.divider_height | default: 1 }}px;
                    min-height: {{ block.settings.divider_height | default: 1 }}px;
                    min-width: 20px;
                    background-color: {{ block.settings.divider_color | default: '#e0e0e0' }};
                    border: none;
                    margin: 0;
                    display: block;
                    {% if block.settings.divider_style == 'dashed' %}
                      background-image: repeating-linear-gradient(
                        to right,
                        {{ block.settings.divider_color | default: '#e0e0e0' }} 0px,
                        {{ block.settings.divider_color | default: '#e0e0e0' }} 8px,
                        transparent 8px,
                        transparent 16px
                      );
                      background-color: transparent;
                    {% elsif block.settings.divider_style == 'dotted' %}
                      background-image: repeating-linear-gradient(
                        to right,
                        {{ block.settings.divider_color | default: '#e0e0e0' }} 0px,
                        {{ block.settings.divider_color | default: '#e0e0e0' }} 4px,
                        transparent 4px,
                        transparent 8px
                      );
                      background-color: transparent;
                    {% endif %}
                  }
                  {% endif %}
                </style>
                <div class="upsell-header-wrapper upsell-header-wrapper-{{ block.id }}" {{ block.shopify_attributes }}>
                  {%- assign header_alignment = block.settings.header_alignment | default: 'center' -%}
                  
                  {% if block.settings.show_divider %}
                    <div class="upsell-header-with-divider">
                      {% if header_alignment == 'right' %}
                        <div class="divider-line"></div>
                      {% elsif header_alignment == 'center' %}
                        <div class="divider-line"></div>
                      {% endif %}
                      
                      <div
                        class="upsell-header-text"
                        style="
                          color: {{ block.settings.header_text_color | default: '#333' }};
                          font-size: {{ block.settings.header_font_size | default: 14 }}px;
                          text-align: {{ header_alignment }};
                          margin: 0 !important;
                          padding: 0 !important;
                          white-space: nowrap;
                        "
                      >
                        {{ block.settings.header_text }}
                      </div>
                      
                      {% if header_alignment == 'left' or header_alignment == 'center' %}
                        <div class="divider-line"></div>
                      {% endif %}
                    </div>
                  {% else %}
                    <div
                      class="upsell-header-text"
                      style="
                        color: {{ block.settings.header_text_color | default: '#333' }};
                        font-size: {{ block.settings.header_font_size | default: 14 }}px;
                        text-align: {{ header_alignment }};
                        margin-top: {{ block.settings.header_margin_top | default: 8 }}px !important;
                        margin-bottom: {{ block.settings.header_margin_bottom | default: 8 }}px !important;
                        margin-left: 0 !important;
                        margin-right: 0 !important;
                        padding: 0 !important;
                        {% if header_alignment == 'left' %}padding-left: 13px !important; padding-right: 13px !important;{% endif %}
                      "
                    >
                      {{ block.settings.header_text }}
                    </div>
                  {% endif %}
                </div>
              {%- endif -%}

              {%- break -%}
            {%- endif -%}
          {%- endfor -%}

          {%- if upsell_carousel_mode -%}
            <div class="upsell_block_container">
          {%- endif -%}

          {%- for block in section.blocks -%}
            {%- if block.type == 'cart_upsell' -%}
              {%- assign upsell_handle = block.settings.upsell_product_1 -%}
              {%- if upsell_handle != blank -%}
                {%- assign upsell_product = all_products[upsell_handle] -%}
                {%- if upsell_product -%}
                  {%- assign upsell_count = upsell_count | plus: 1 -%}
                  {% if cart != empty %}
                    {%- liquid 
                      assign product_form_id = 'cart-upsell-form-' | append: block.id
                      assign this = block.settings
                      assign upsell_product_in_cart = false
                      assign upsell_product_id = this.upsell_product_1.id
                      assign check_upsell_product_in_cart = cart.items | where: 'product_id', upsell_product_id | first

                      if check_upsell_product_in_cart
                        assign upsell_product_in_cart = true
                        assign upsell_count = upsell_count | minus: 1 
                      endif
                    -%}

                    {%- unless upsell_product_in_cart -%}
                      {%-
                        render 'cart-drawer-upsell',
                        block_id: block.id,
                        product_form_id: product_form_id,
                        upsell_product: upsell_product,
                        show_description: block.settings.show_upsell_description,
                        description_text: block.settings.upsell_description_text,
                        upsell_button_text: block.settings.upsell_button_text,
                        button_text_color: block.settings.upsell_button_text_color,
                        upsell_custom_bestseller_svg: block.settings.upsell_custom_bestseller_svg,
                        title_color: block.settings.upsell_title_color,
                        description_color: block.settings.upsell_description_color,
                        show_best_seller_tag: block.settings.show_upsell_best_seller_tag,
                        best_seller_text: block.settings.upsell_best_seller_text,
                        best_seller_tag_color: block.settings.upsell_best_seller_tag_color,
                        best_seller_tag_text_color: block.settings.upsell_best_seller_tag_text_color,
                        show_price: block.settings.upsell_show_price,
                        card_bg_color: block.settings.upsell_card_background_color,
                        card_border_radius: block.settings.upsell_card_border_radius,
                        border_width: block.settings.upsell_border_width,
                        border_style: block.settings.upsell_border_style,
                        border_color: block.settings.upsell_border_color,
                        price_color: block.settings.upsell_price_color,
                      variant_border_color: block.settings.upsell_variant_border_color,
                        variant_border_radius: block.settings.upsell_variant_border_radius,
                        variant_bg_color: block.settings.upsell_variant_background_color,
                        variant_text_color: block.settings.upsell_variant_text_color,
                        button_bg_color: block.settings.upsell_button_bg_color,
                        button_border_radius: block.settings.upsell_button_border_radius,
                        button_text_color: block.settings.upsell_button_text_color,
                        upsell_show_price: block.settings.upsell_show_price,
                        remove_container_border: block.settings.upsell_remove_container_border,
                        image_border_radius: block.settings.upsell_image_border_radius,
                        product_image_bg_color: block.settings.upsell_product_image_background_color,
                        title_font_size: block.settings.upsell_title_font_size,
                        description_font_size: block.settings.upsell_description_font_size,
                        price_font_size: block.settings.upsell_price_font_size,
                        variant_font_size: block.settings.upsell_variant_font_size,
                        button_font_size: block.settings.upsell_button_font_size,
                        tag_font_size: block.settings.upsell_tag_font_size,
                      top_margin: block.settings.upsell_margin_top,
                      bottom_margin: block.settings.upsell_margin_bottom
                      -%}
                    {%- endunless -%}
                  {% endif %}
                {%- endif -%}
              {%- endif -%}
            {%- endif -%}
          {%- endfor -%}

          {%- if upsell_carousel_mode -%}
            </div>
            {%- if upsell_count > 1 -%}
              <div class="dots-and-arrows">
                <button id="slide-arrow-prev" class="slide-arrow prev" type="button" aria-label="Prev">
                  <svg viewBox="0 0 20 20">
                    <path d="M14 5l-5 5 5 5-1 2-7-7 7-7z"/>
                  </svg>
                </button>

                {%- for i in (1..upsell_count) -%}
                  <span
                    class="slide_item{% if forloop.first %} active{% endif %}"
                    data-slide="{{ forloop.index }}"
                  ></span>
                {%- endfor -%}

                <button id="slide-arrow-next" class="slide-arrow next" type="button" aria-label="Next">
                  <svg viewBox="0 0 20 20">
                    <path d="M6 15l5-5-5-5 1-2 7 7-7 7z"/>
                  </svg>
                </button>
              </div>
            {%- endif -%}
          {%- endif -%}
        </div>
      </cart-drawer-items>

      <div class="drawer__footer">
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'cart_note' %}
              {%- if block.settings.show_cart_note -%}
                <details id="Details-CartDrawer" style="padding: 0 15px;">
                  <summary>
                    <span class="summary__title">
                      {{ 'sections.cart.note' | t }}
                      {{- 'icon-caret.svg' | inline_asset_content -}}
                    </span>
                  </summary>
                  <cart-note class="cart__note field">
                    <label class="visually-hidden" for="CartDrawer-Note">{{ 'sections.cart.note' | t }}</label>
                    <textarea
                      id="CartDrawer-Note"
                      class="text-area text-area--resize-vertical field__input"
                      name="note"
                      placeholder="{{ 'sections.cart.note' | t }}"
                    >{{ cart.note }}</textarea>
                  </cart-note>
                </details>
              {%- endif -%}

              <!-- Start blocks -->
              <!-- Discount Code -->
            {% when 'cart_discount_code' %}
              {% if cart != empty %}
                {% render 'cart-discount-code', block: block %}
              {% endif %}

              <!-- Subtotals -->
            {% when 'cart_subtotals' %}
              <div class="cart-drawer__footer" style="display:none;" {{ block.shopify_attributes }}>
                <div>
                  {%- if cart.cart_level_discount_applications.size > 0 -%}
                    <ul class="discounts list-unstyled" role="list" aria-label="{{ 'customer.order.discount' | t }}">
                      {%- for discount in cart.cart_level_discount_applications -%}
                        <li class="discounts__discount discounts__discount--end">
                          {{- 'icon-discount.svg' | inline_asset_content -}}
                          {{ discount.title | escape }}
                          (-{{ discount.total_allocated_amount | money }})
                        </li>
                      {%- endfor -%}
                    </ul>
                  {%- endif -%}
                </div>

                <div class="totals" role="status">
                  <h2 class="totals__total">
                    <p class="gb-totals-total-value" style="display:none;">{{ cart.total_price }}</p>
                    {{ 'sections.cart.estimated_total' | t }}
                  </h2>

                  <p class="totals__total-value">{{ cart.total_price | money_with_currency }}</p>
                </div>

                <small class="tax-note caption-large rte">
                  {%- if cart.duties_included and cart.taxes_included -%}
                    {%- if shop.shipping_policy.body == blank -%}
                      {{ 'sections.cart.duties_and_taxes_included_shipping_at_checkout_without_policy' | t }}
                    {%- else -%}
                      {{
                        'sections.cart.duties_and_taxes_included_shipping_at_checkout_with_policy_html'
                        | t: link: shop.shipping_policy.url
                      }}
                    {%- endif -%}
                  {%- elsif cart.duties_included == false and cart.taxes_included -%}
                    {%- if shop.shipping_policy.body == blank -%}
                      {{ 'sections.cart.taxes_included_shipping_at_checkout_without_policy' | t }}
                    {%- else -%}
                      {{
                        'sections.cart.taxes_included_shipping_at_checkout_with_policy_html'
                        | t: link: shop.shipping_policy.url
                      }}
                    {%- endif -%}
                  {%- elsif cart.duties_included and cart.taxes_included == false -%}
                    {%- if shop.shipping_policy.body == blank -%}
                      {{ 'sections.cart.duties_included_taxes_at_checkout_shipping_at_checkout_without_policy' | t }}
                    {%- else -%}
                      {{
                        'sections.cart.duties_included_taxes_at_checkout_shipping_at_checkout_with_policy_html'
                        | t: link: shop.shipping_policy.url
                      }}
                    {%- endif -%}
                  {%- elsif cart.duties_included == false and cart.taxes_included == false -%}
                    {%- if shop.shipping_policy.body == blank -%}
                      {{ 'sections.cart.taxes_at_checkout_shipping_at_checkout_without_policy' | t }}
                    {%- else -%}
                      {{
                        'sections.cart.taxes_at_checkout_shipping_at_checkout_with_policy_html'
                        | t: link: shop.shipping_policy.url
                      }}
                    {%- endif -%}
                  {%- endif -%}
                </small>
              </div>
            {% when 'cart_shipping_protection' %}
              {% render 'cart_shipping_protection_style', block: block %}
              {% capture get_shipping_pro_id %}
              {% assign found = false %}
              {%- for item in cart.items -%}
                {% if item.id == block.settings.product_shipping_protection.selected_or_first_available_variant.id %}
                  {% assign found = true %}
                  checked
                  {% break %}
                {% endif %}
              {%- endfor -%}
              {% if found == false %}
                unchecked
              {% endif %}
            {% endcapture %}
              <product-form
                style="display:none;"
                class="product-form gb-btn gbgbgb"
                data-hide-errors="{{ gift_card_recipient_feature_active }}"
                data-section-id="{{ section.id }}"
              >
                <div class="product-form__error-message-wrapper" role="alert" hidden>
                  <svg
                    aria-hidden="true"
                    focusable="false"
                    class="icon icon-error"
                    viewBox="0 0 13 13"
                  >
                    <circle cx="6.5" cy="6.50049" r="5.5" stroke="white" stroke-width="2"/>
                    <circle cx="6.5" cy="6.5" r="5.5" fill="#EB001B" stroke="#EB001B" stroke-width="0.7"/>
                    <path d="M5.87413 3.52832L5.97439 7.57216H7.02713L7.12739 3.52832H5.87413ZM6.50076 9.66091C6.88091 9.66091 7.18169 9.37267 7.18169 9.00504C7.18169 8.63742 6.88091 8.34917 6.50076 8.34917C6.12061 8.34917 5.81982 8.63742 5.81982 9.00504C5.81982 9.37267 6.12061 9.66091 6.50076 9.66091Z" fill="white"/>
                    <path d="M5.87413 3.17832H5.51535L5.52424 3.537L5.6245 7.58083L5.63296 7.92216H5.97439H7.02713H7.36856L7.37702 7.58083L7.47728 3.537L7.48617 3.17832H7.12739H5.87413ZM6.50076 10.0109C7.06121 10.0109 7.5317 9.57872 7.5317 9.00504C7.5317 8.43137 7.06121 7.99918 6.50076 7.99918C5.94031 7.99918 5.46982 8.43137 5.46982 9.00504C5.46982 9.57872 5.94031 10.0109 6.50076 10.0109Z" fill="white" stroke="#EB001B" stroke-width="0.7">
                  </svg>
                  <span class="product-form__error-message"></span>
                </div>

                <form
                  method="post"
                  action="{{ routes.cart_add_url }}"
                  id="product-form-template--shipping-protection__main"
                  accept-charset="UTF-8"
                  class="form shipping-protection-form"
                  enctype="multipart/form-data"
                  novalidate="novalidate"
                  data-type="add-to-cart-form"
                >
                  <input
                    type="hidden"
                    name="id"
                    value="{{block.settings.product_shipping_protection.selected_or_first_available_variant.id}}"
                    class="product-variant-id"
                  >
                  <div class="product-form__buttons">
                    {%- liquid
                      assign check_against_inventory = true
                      if block.settings.product_shipping_protection.selected_or_first_available_variant.inventory_management != 'shopify' or block.settings.product_shipping_protection.selected_or_first_available_variant.inventory_policy == 'continue'
                        assign check_against_inventory = false
                      endif
                      if block.settings.product_shipping_protection.selected_or_first_available_variant.quantity_rule.min > block.settings.product_shipping_protection.selected_or_first_available_variant.inventory_quantity and check_against_inventory
                        assign quantity_rule_soldout = true
                      endif
                    -%}
                    <button
                      id="ProductSubmitButton-{{ block.id }}"
                      type="submit"
                      name="add"
                      class="gb-product-shipping-protection-product-tirgger product-form__submit button button--full-width {% if show_dynamic_checkout %}button--secondary{% else %}button--primary{% endif %}"
                      {% if block.settings.product_shipping_protection.selected_or_first_available_variant.available
                          == false
                        or quantity_rule_soldout
                      %}
                        disabled
                      {% endif %}
                    >
                      <span>
                        {%- if block.settings.product_shipping_protection.selected_or_first_available_variant.available
                            == false
                          or quantity_rule_soldout
                        -%}
                          {{ 'products.product.sold_out' | t }}
                        {%- else -%}
                          {{ 'products.product.add_to_cart' | t }}
                        {%- endif -%}
                      </span>
                      {%- render 'loading-spinner' -%}
                    </button>
                    {%- if show_dynamic_checkout -%}
                      {{ form | payment_button }}
                    {%- endif -%}
                  </div>
                </form>
              </product-form>
              <div class="gb-shipping-protection">
                <div class="gb-shipping-protection-icon">
                  {% if block.settings.cart_drawer_sp_icon != blank -%}
                    <img
                      src="{{ block.settings.cart_drawer_sp_icon | image_url: width: 92 }}"
                      srcset="{{ block.settings.cart_drawer_sp_icon | image_url: width: 92 }} 1x, {{ block.settings.cart_drawer_sp_icon | image_url: width: 184 }} 2x, {{ block.settings.cart_drawer_sp_icon | image_url: width: 276 }} 3x"
                      alt="{{ block.settings.cart_drawer_sp_heading | default: 'Shipping Protection' | escape }}"
                      width="46"
                      height="46"
                      loading="lazy"
                      style="object-fit: contain; image-rendering: -webkit-optimize-contrast; image-rendering: crisp-edges;"
                    >
                  {% else %}
                    <svg
                      width="46"
                      height="46"
                      viewBox="0 0 46 46"
                      fill="none"
                      xmlns="http://www.w3.org/2000/svg"
                      xmlns:xlink="http://www.w3.org/1999/xlink"
                    >
                      <rect width="46" height="46" fill="url(#pattern0_1_81)"/>
                      <defs>
                      <pattern id="pattern0_1_81" patternContentUnits="objectBoundingBox" width="1" height="1">
                      <use xlink:href="#image0_1_81" transform="scale(0.00195312)"/>
                      </pattern>
                      <image id="image0_1_81" width="512" height="512" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
                      </defs>
                    </svg>
                  {% endif %}
                </div>
                <div class="gb-shipping-protection-icontext">
                  <h2>
                    {{ block.settings.cart_drawer_sp_heading | default: 'Shipping Protection' | escape }}
                    <span>
                      {{-
                        block.settings.product_shipping_protection.selected_or_first_available_variant.price
                        | money
                      -}}
                    </span>
                  </h2>
                  <p>
                    {{
                      block.settings.cart_drawer_sp_text
                      | default: 'Protect your order from damage, loss, or theft during shipping'
                      | escape
                    }}
                  </p>
                </div>
                <div class="gb-shipping-protection-button">
                  <label class="switch {{ get_shipping_pro_id }}">
                    <input type="checkbox" {{ get_shipping_pro_id }}>
                    <span class="slider round"></span>
                    <div class="spin-animation border-gray-300 h-20 w-20 animate-spin rounded-full border-8 border-t-blue-600">
                      <span></span>
                    </div>
                    <div class="complete-check">
                      <svg fill="#fff" viewBox="0 0 1920 1920" xmlns="http://www.w3.org/2000/svg">
                        <path d="M1743.858 267.012 710.747 1300.124 176.005 765.382 0 941.387l710.747 710.871 1209.24-1209.116z" fill-rule="evenodd"/>
                      </svg>
                    </div>
                  </label>
                </div>
              </div>

              <!-- CTAs -->
          {% endcase %}
        {% endfor %}

        <div
          class="totals gb-discounts-cart-values"
          role="status"
          style="display: flex; justify-content: space-between; align-items: end; margin-bottom: 5px; padding: 0 0px;"
        >
          <div class="totals__total">
            <span class="total_count">
              {{ 'sections.cart.items_total' | t: item_count: cart.item_count }}
            </span>
          </div>
          <div class="discount_price-container">
            {% if cart.total_discount != 0 %}
              <div class="discount_discount-value" style="margin-left: 8px;">
                {% if total_discount_msg != '' %}
                  <span class="discount_discount-label">{{ total_discount_msg }}</span>
                {% endif %}
                <span style="font-weight: var(--font-weight-regular);">-{{ cart.total_discount | money }}</span>
              </div>
            {% endif %}
            {%- liquid
              assign cart_total_compare_at_price = 0
              for item_loop_var in cart.items
                assign item_price_for_calc = item_loop_var.variant.price
                assign item_display_price_before_any_discount = item_price_for_calc
                if item_loop_var.variant.compare_at_price and item_loop_var.variant.compare_at_price > item_price_for_calc
                  assign item_display_price_before_any_discount = item_loop_var.variant.compare_at_price
                endif
                assign line_total_compare_price = item_display_price_before_any_discount | times: item_loop_var.quantity
                assign cart_total_compare_at_price = cart_total_compare_at_price | plus: line_total_compare_price
              endfor
            -%}
            <div style="display: flex; align-items: baseline;">
              {% if cart_total_compare_at_price > cart.total_price %}
                <s
                  class="cart-item__old-price product-option"
                  style="margin-right: 3px; font-size: 14px; color: currentColor; opacity: 0.7;"
                >
                  {{ cart_total_compare_at_price | money }}
                </s>
              {% endif %}

              <div class="totals__total-value">
                {{ cart.total_price | money }}
              </div>
            </div>
          </div>
          <p class="gb-totals-total-value" style="display:none;">{{ cart.total_price }}</p>
        </div>
        <div class="cart__ctas" {{ block.shopify_attributes }}>
          <button
            type="submit"
            id="CartDrawer-Checkout"
            class="cart__checkout-button button"
            name="checkout"
            form="CartDrawer-Form"
            {% if cart == empty %}
              disabled
            {% endif %}
          >
            <svg
              class="icon icon-lock"
              aria-hidden="true"
              focusable="false"
              role="presentation"
              xmlns="http://www.w3.org/2000/svg"
              viewBox="0 0 24 24"
              fill="none"
              stroke="currentColor"
              stroke-width="2"
              stroke-linecap="round"
              stroke-linejoin="round"
            >
              <rect x="3" y="11" width="18" height="11" rx="2" ry="2"></rect>
              <path d="M7 11V7a5 5 0 0 1 10 0v4"></path>
            </svg>
            <span class="button-text">
              {{- section.settings.cart_drawer_checkout_button_label | default: 'SECURE CHECKOUT' | escape -}}
            </span>
          </button>
        </div>
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'cart_money_back_guarantee' %}
              {% if block.settings.show_money_back_guarantee != false %}
                {% render 'cart-money_back_guarantee', block: block %}
              {% endif %}
            {% when 'cart_payment_icon' %}
              <div class="cart-drawer-payment-icons">
                {% render 'payment-icons', block: block %}
              </div>
          {% endcase %}
        {% endfor %}
      </div>
    </div>
  </div>
</cart-drawer>

