<section class="custom-hero section-{{ section.id }} page-fade-in">
  <div class="custom-hero__container">
      <div class="custom-hero__flex custom-hero__flex--image-{{ section.settings.image_position }}">
        <!-- Product Image -->
        <div class="custom-hero__image-wrapper">
        {% if section.settings.hero_video != blank %}
          {% unless section.settings.use_background_image and section.settings.hero_background_image != blank %}
            <!--
              Video Implementation Notes:
              - No poster attribute to prevent image flash
              - Video starts hidden (opacity: 0) and fades in when loaded
              - Fallback image only shows if video fails to load
              - Loading spinner shows while video loads
              - Preload helps with faster loading
            -->
            <link
              rel="preload"
              as="video"
              href="{{ section.settings.hero_video.sources[1].url }}"
              type="video/mp4">

            <div class="custom-hero__video-container">
              <video
                class="custom-hero__video"
                autoplay
                muted
                loop
                playsinline
                preload="auto"
                webkit-playsinline
                x5-playsinline>
                <source src="{{ section.settings.hero_video.sources[1].url }}" type="video/mp4">
              </video>
              <!-- Fallback image that only shows if video fails to load -->
              {% if section.settings.hero_image %}
                {{
                  section.settings.hero_image | image_url: width: 800 | image_tag: class: 'custom-hero__video-fallback', loading: 'lazy', alt: section.settings.heading
                }}
              {% endif %}
            </div>
          {% endunless %}
        {% elsif section.settings.hero_image %}
          {% unless section.settings.use_background_image and section.settings.hero_background_image != blank %}
            {{
              section.settings.hero_image | image_url: width: 800 | image_tag: class: 'custom-hero__image', loading: 'lazy', alt: section.settings.heading
            }}
          {% endunless %}
         {% else %}
           {% unless section.settings.use_background_image and section.settings.hero_background_image != blank %}
             {{ 'image' | placeholder_svg_tag: 'custom-hero__image placeholder' }}
           {% endunless %}
         {% endif %}
       </div>

      <!-- Product Content -->
      <div class="custom-hero__content">
        <div class="custom-hero__content-wrapper">
          {% for block in section.blocks %}
            {% case block.type %}
              {% when 'badge' %}
                <div
                  class="custom-hero__badge-wrapper"
                  data-block-id="{{ block.id }}"
                  {{ block.shopify_attributes }}
                >
                  <div class="custom-hero__badge">{{ block.settings.badge_text }}</div>
                </div>

              {% when 'rating' %}
                <div
                  class="custom-hero__rating-wrapper{% if block.settings.enable_container_background %} has-container-background{% endif %}"
                  data-block-id="{{ block.id }}"
                  {{ block.shopify_attributes }}
                >
                  <div class="custom-hero__rating-container">
                    {% if block.settings.profile_image_1 != blank
                      or block.settings.profile_image_2 != blank
                      or block.settings.profile_image_3 != blank
                    %}
                      <div class="custom-hero__profile-images">
                        {% if block.settings.profile_image_1 %}
                          {{
                            block.settings.profile_image_1
                            | image_url: width: 120
                            | image_tag: class: 'custom-hero__profile-image', loading: 'lazy', alt: 'Profile Image'
                          }}
                        {% endif %}
                        {% if block.settings.profile_image_2 %}
                          {{
                            block.settings.profile_image_2
                            | image_url: width: 120
                            | image_tag: class: 'custom-hero__profile-image', loading: 'lazy', alt: 'Profile Image'
                          }}
                        {% endif %}
                        {% if block.settings.profile_image_3 %}
                          {{
                            block.settings.profile_image_3
                            | image_url: width: 120
                            | image_tag: class: 'custom-hero__profile-image', loading: 'lazy', alt: 'Profile Image'
                          }}
                        {% endif %}
                      </div>
                    {% endif %}
                    <div class="custom-hero__rating-stars">
                      {% if section.settings.use_theme_colors %}
                        {% comment %} Show 5 full stars when theme colors are on {% endcomment %}
                        {% for i in (1..5) %}
                          {% render 'icons', icon: 'custom_hero_rating' %}
                        {% endfor %}
                      {% else %}
                        {% comment %} Show 5 full stars when theme colors are off {% endcomment %}
                        {% for i in (1..5) %}
                          {% render 'icons', icon: 'custom_hero_rating' %}
                        {% endfor %}
                      {% endif %}
                    </div>
                    <div class="custom-hero__rating-text">{{ block.settings.rating_text }}</div>
                  </div>
                </div>

              {% when 'heading' %}
                <h2
                  class="custom-hero__title section-heading"
                  data-block-id="{{ block.id }}"
                  {{ block.shopify_attributes }}
                >
                  {%- if block.settings.heading != blank -%}
                    {{ block.settings.heading }}
                  {%- endif -%}
                  {%- if block.settings.heading_accent != blank -%}
                    <span class="section-accent-text custom-hero-accent{% if block.settings.use_theme_colors == false and block.settings.use_accent_gradient and block.settings.accent_gradient != blank %} accent-gradient-enabled{% else %} no-gradient{% endif %}">
                      {{- block.settings.heading_accent -}}
                    </span>
                  {%- endif -%}
                </h2>

              {% when 'subtitle' %}
                {% if block.settings.subtitle != blank %}
                  <div
                    class="custom-hero__subtitle"
                    data-block-id="{{ block.id }}"
                    {{ block.shopify_attributes }}
                  >
                    {{ block.settings.subtitle }}
                  </div>
                {% endif %}

              {% when 'benefits' %}
                <div
                  class="custom-hero__benefits{% if block.settings.benefits_flex_layout %} flex-layout{% endif %}"
                  data-block-id="{{ block.id }}"
                  {{ block.shopify_attributes }}
                >
                  {% if block.settings.benefit_1_text != blank %}
                    <div class="custom-hero__benefit-item">
                      <span class="custom-hero__benefit-icon">
                        {% case block.settings.benefit_1_icon %}
                          {% when 'check' %}
                            {{ checkmark_icon }}
                          {% when 'badge_check' %}
                            {{ badge_checkmark_icon }}
                          {% when 'circle_check' %}
                            {{ circle_checkmark_icon }}
                          {% when 'filled_circle_check' %}
                            {{ filled_circle_checkmark_icon }}
                          {% when 'check_mark' %}
                            {{ check_icon }}
                          {% when 'custom' %}
                            {{ block.settings.benefit_1_custom_svg }}
                          {% when 'custom_image' %}
                            {% if block.settings.benefit_1_custom_image != blank %}
                              {{ block.settings.benefit_1_custom_image | image_url: width: 100 | image_tag: loading: 'lazy', alt: block.settings.benefit_1_text }}
                            {% endif %}
                        {% endcase %}
                      </span>
                      <span>{{ block.settings.benefit_1_text }}</span>
                    </div>
                  {% endif %}

                  {% if block.settings.benefit_2_text != blank %}
                    <div class="custom-hero__benefit-item">
                      <span class="custom-hero__benefit-icon">
                        {% case block.settings.benefit_2_icon %}
                          {% when 'check' %}
                            {{ checkmark_icon }}
                          {% when 'badge_check' %}
                            {{ badge_checkmark_icon }}
                          {% when 'circle_check' %}
                            {{ circle_checkmark_icon }}
                          {% when 'filled_circle_check' %}
